import networkx as nx
import matplotlib.pyplot as plt
import random
X = random.randrange(3,10)

G = nx.DiGraph()
for i in range(1,X):
    G.add_node(i+1)
    

for i in range(X+1):
    lig = random.randint(0,1)
    for j in range(X):
        if j==i:
            lig = 0
        if j==X//2:
            G.add_edge(i,j)
        if j != i and j!= X/2:
            if lig == 1:
                G.add_edge(i,j)
            




nx.draw(G, with_labels=1)
plt.show()
